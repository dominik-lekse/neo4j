<?xml version="1.0" encoding="UTF-8"?>
<actions>
    <action>
        <actionName>build</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>install</goal>
        </goals>
        <properties>
            <minimalBuild>true</minimalBuild>
            <skipTests>true</skipTests>
            <licensing.skip>true</licensing.skip>
            <Env.MAVEN_OPTS>-Xmx2048m</Env.MAVEN_OPTS>
        </properties>
    </action>
    <action>
        <actionName>CUSTOM-Build project (resume neo4j-browser)</actionName>
        <displayName>Build project (resume neo4j-browser)</displayName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>-rf :neo4j-browser install</goal>
        </goals>
        <properties>
            <minimalBuild>true</minimalBuild>
            <skipTests>true</skipTests>
            <licensing.skip>true</licensing.skip>
            <Env.MAVEN_OPTS>-Xmx2048m</Env.MAVEN_OPTS>
        </properties>
    </action>
    <action>
        <actionName>CUSTOM-Build project (resume community-build)</actionName>
        <displayName>Build project (resume community-build)</displayName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>-rf :community-build install</goal>
        </goals>
        <properties>
            <minimalBuild>true</minimalBuild>
            <skipTests>true</skipTests>
            <licensing.skip>true</licensing.skip>
            <Env.MAVEN_OPTS>-Xmx2048m</Env.MAVEN_OPTS>
        </properties>
    </action>
</actions>
